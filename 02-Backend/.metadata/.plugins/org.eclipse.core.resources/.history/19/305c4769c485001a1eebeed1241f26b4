Integer [] a= {1,2};
Integer [] b= {2,3};
Integer [] c= {1,3};
List<Integer[]>fountain_list=new ArrayList<Integer[]>();
fountain_list.add(a);
fountain_list.add(b);
fountain_list.add(c);

Collections.sort(fountain_list,new BoundarySorter());

System.out.print(fountain_list);


}
}
