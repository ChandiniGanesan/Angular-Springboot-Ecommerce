import java.util.*;

class collectionsMin {
	static class BoundarySorter implements Comparator<Integer[]>{
		 public int compare(Integer[] boundary1, Integer[] boundary2){
		 int result = 0;
		 if(boundary1[0] != boundary2[0]){
		 result = boundary1[0] - boundary2[0];
		 }else{
		 result = boundary2[1] - boundary1[1];
		 }
		 return result;
		 }
		 }
public static void main(String[] args) {
/*	
